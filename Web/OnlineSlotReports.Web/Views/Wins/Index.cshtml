@model OnlineSlotReports.Web.ViewModels.WinsViewModels.AllWinsViewModel
@{
    ViewData["Title"] = "AllWins";
}

<h1 align="center" class="display-4">All Wins</h1>
<div class="row">
    @foreach (var win in Model.Wins)
    {
        <div class="col-md-4 card" style="width: 18rem;">
            <img class="card-img-top" src="@win.Url" alt="Card image cap" height="200">
            <div class="card-body">
                <h5 class="card-title">@win.Date</h5>
                <p class="card-text">@win.Description </p>
            </div>
        </div>
    }
</div>
@section Links{

    <li class="nav-item">
        <a class="nav-link text-dark" href="/SlotMachine/Index/@Model.GamingHallId">Slot machines</a>
    </li>
    <li class="nav-item">
        <a class="nav-link text-dark" href="/Gallery/Index/@Model.GamingHallId">Gallery</a>
    </li>
    <li class="nav-item">
        <a class="nav-link text-dark" href="/Wins/Index/@Model.GamingHallId">Wins</a>
    </li>
    <li class="nav-item">
        <a class="nav-link text-dark" href="/GamingHall/Details/@Model.GamingHallId">Contacts</a>
    </li>

}